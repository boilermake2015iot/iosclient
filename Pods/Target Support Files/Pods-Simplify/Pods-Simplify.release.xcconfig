EMBEDDED_CONTENT_CONTAINS_SWIFT = YES
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$CONFIGURATION_BUILD_DIR/ChameleonFramework.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/ColorUtils.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/JHColorPicker.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/MRProgress.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/UIColor_Crayola.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/UIColor_Pantone.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "ChameleonFramework" -framework "ColorUtils" -framework "JHColorPicker" -framework "MRProgress" -framework "UIColor_Crayola" -framework "UIColor_Pantone"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-Simplify
PODS_ROOT = ${SRCROOT}/Pods